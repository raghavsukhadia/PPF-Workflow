{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/dist/public/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}