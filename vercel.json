{
    "version": 2,
    "outputDirectory": "dist/public",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.ts"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}